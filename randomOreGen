--Sets up the number of ore and directories
local numOfOre = 3
local baseOre =
local listOfPos = {
    {}
}


--Spawns a new ore when called
function spawnOre()
	local randomPos = listOfPos[math.Random(x, y)]
	local x = randomPos[1]
	local y = randomPos[2]
	local z = randomPos[3]
		local newOre = baseOre:Clone()
			warn("Current random coordinates are " .. x, y, z)
			warn("Step 1 Passed")
		newOre.Parent = workspace.
			warn("Step 2 Passed")
		newOre.Position = Vector3.new(x, y, z)
			warn("Step 3 Passed")
		print("Successfully created new Ore (location)")
			warn("Step 4 Passed")
		wait(0.1)
end


--Repeats function for how many you set numOfOre to
for i = 1, numOfOre do
	spawnOre()
end
